#include<iostream>
using namespace std;
int main(){
    int num,flag=1;
    cin>>num;
    if(num>0){
        for(int i=2;i<num;i++){
            if(num%i== 0){
                flag=0;
                break;
            }
    }
    }
    if(flag==0){
        cout<<num<<" is not a prime number";
    }
    else{
        cout<<num<<" is a prime number";
    }
    return 0;
}
