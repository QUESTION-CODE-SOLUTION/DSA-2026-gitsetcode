//code to print table using loops
#include<iostream>
using namespace std;
 int main()
 {
    int a;
    cin>>a;
    for(int i=1;i<=10;i++){
        cout<<a<<"*"<<i<<"="<<a*i<<endl;
    }
    cout<<endl;
    cout<<"\n";// endl & \n both are same
    for(int i=10;i>0;i--){
        cout<<a<<"*"<<i<<"="<<a*i<<endl;
    }
    return 0;
}

// how loops work
#include<iostream>
using namespace std;
 int main()
 {
    for(int i=1;i<11;i++){
        for(int j=1;j<11;j++){
            cout<<i<<" "<<j<<" "<<endl;;
        }
    }
    return 0;
}

