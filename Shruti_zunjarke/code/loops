#include<iostream>
using namespace std;
int main(){
    for(int i=1;i<=5;i++){
        for(int k=1;k<=i;k++){
            cout<<"* ";
        }
        cout<<endl;
    }
    
    for(int l=1;l<=4;l++){
        for(int j=l;j<=4;j++){
            cout<<"* ";
        }
        cout<<endl;
    }
    return 0;
}

