#include<iostream>
using namespace std;
 int main()
 {
    for(int i=1;i<=4;i++){ 
        for(int j=1;j<=5;j++){
            cout<<i<<" ";
        }
        cout<<endl;
    }
    return 0;
}

/*
* # * # 
* # * # 
* # * # 
* # * # 
*/
#include<iostream>
using namespace std;
 int main()
 {
    for(int i=1;i<=4;i++){ 
        for(int j=1;j<=4;j++){
            if(j%2==0){
                cout<<"#"<<" ";
            }
            else{
                cout<<"*"<<" ";
            }
        }
        cout<<endl;
    }
    return 0;
}

