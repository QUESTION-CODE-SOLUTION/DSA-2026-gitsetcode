// ip_op 
#include<iostream>
using namespace std;

int main() {
	char a;
	cin>>a;
        if (a>='A'&&a<='Z') {
          cout << 1;
        } else if (a>='a'&&a<='z') {
          cout << 0;
        }
		else{
			cout<<-1;
		}
}

//Datatype_size
#include<iostream>
using namespace std;
int dataTypes(string type) {
	if(type=="Integer"){
		return sizeof(int);
	}
	else if(type=="Long"){
		return sizeof(long);
	}
	else if(type=="Float"){
		return sizeof(float);
	}
	else if(type=="Double"){
		return sizeof(double);
	}
	else if(type=="Character"){
		return sizeof(char);
	}
	else{

	}

}
int main(){
    string type;
    cin>>type;
    cout<<dataTypes(type);
}

//If/else conditional statements
#include<iostream>
using namespace std;
string compareIfElse(int a, int b) {
	if(a>b){
		return "greater";
	}
	else if(a<b){
		return "smaller";
	}
	else{
		return "equal";
	}
}
int main(){
    int a,b;
    cin>>a>>b;
    cout<<compareIfElse(a,b);
}

//Switch statements
#include<iostream>
#include <vector>
#include <cmath>
using namespace std;

double areaSwitchCase(int ch, vector<double> a) {
	switch(ch){
		case 1:
		return M_PI*a[0]*a[0];
		case 2:
		return a[0]*a[1];
	}
}
int main(){
    vector<double> a;
    int ch;
    cin>>ch;
    for(int i=0;i<a.size();i++){
        cin>>a[i];
    }
    cout<<areaSwitchCase(ch, a);
}
