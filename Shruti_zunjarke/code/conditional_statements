#include<iostream>
using namespace std;
 int main()
 {
    int age;
    cout<<"Enter your age: ";
    cin>>age;
    if(age>0 && age<=18)
    {
        cout<<"School student"<<endl;
    }
    else if(age>18 && age<=22)
    {
        cout<<"You are doing your graduation"<<endl;

    }
    else if(age>22 && age<35)
    {
        cout<<"may be studing"<<endl;
    }
    else{
        cout<<"Free from study"<<endl;
    }
 }
