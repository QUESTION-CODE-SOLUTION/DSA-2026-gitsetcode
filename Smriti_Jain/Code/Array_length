#include <iostream>
using namespace std;
int main()
{
    int n,length;
    int arr[5] = {7,9,1,4};
    length = sizeof(arr) / sizeof(arr[0]);
    cout << "Number of elements: " << length << endl;
    cout << "Elements of Array are:";
    for (int i = 0; i < length; i++)
    {
        cout << arr[i] << " ";
    }
    return 0;
}


// Number of elements: 5
// Elements of Array are:7 9 1 4 0 
