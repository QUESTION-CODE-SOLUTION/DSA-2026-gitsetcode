#include <iostream>
using namespace std;
int main()
{
    int arr[5],sum=0,len;
    float avg;
    len=sizeof(arr)/sizeof(arr[0]);
    cout << "Enter elements of array:" ;
    for(int i=0;i<5;i++){
        cin>>arr[i] ;
        sum+=arr[i];
    }
    avg=sum/len;
    cout << "Average of elements of array is: "<<avg;
	return 0;
}


// Enter elements of array:1 2 3 4 5
// Average of elements of array is: 3
