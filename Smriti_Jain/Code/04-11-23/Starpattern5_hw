// **** ****
// ***   ***
// **     **
// *       *



#include <iostream>
using namespace std;
int main(){
    for(int i=0;i<=4;i++){
        for(int j=-4;j<=4;j++){
            if(j>-i && j<i){
                cout<<" ";
            }
            else{
                cout<<"*";
            }
        }
        cout<<endl;
    }
}
