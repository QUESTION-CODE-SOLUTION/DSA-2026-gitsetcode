#include <iostream>
#include<math.h>
using namespace std;

int main() {
    int num;
    cin>>num;
    int power=0;
    int ans=0;
    while(num!=0){
        int rem=num%2;
        ans=ans+pow(10,power)*rem;
        power++;
        num=num/2;
    }
    cout<<ans;
    

    return 0;
}


// 5
// 101
