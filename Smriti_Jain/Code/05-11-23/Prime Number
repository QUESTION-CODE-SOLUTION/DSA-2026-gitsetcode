#include <iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter a number: ";
    cin>>n;
    for(int i=2;i<n;i++){
        if(n%i==0){
            cout<<n<<" is not a prime no.";
        }
        else{
            cout<<n<<" is a prime no.";
        } 
        break;
        cout<<endl;
    }
    return 0;
}


// Enter a number: 65
// 65 is a prime no.
