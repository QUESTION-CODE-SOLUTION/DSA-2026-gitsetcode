#include <iostream>
using namespace std;
int main() {
    int n,fact=1;
    cout<<"Enter a number: ";
    cin>>n;
    if(n<0){
        cout<<"Factorial doesn't exist!";
    }
    else{
        for(int i=1;i<=n;i++){
        fact=fact*i;
    }
    cout<<"factorial of "<<n<<" is "<<fact;
    }
    return 0;
}



// Enter a number: 5
// factorial of 5 is 120
// Enter a number: -5
// Factorial doesn't exist!
