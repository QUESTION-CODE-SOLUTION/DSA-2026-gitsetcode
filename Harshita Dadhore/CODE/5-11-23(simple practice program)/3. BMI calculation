
#include <iostream> 
#include <math.h> 
using namespace std; 
int main(){
  float height;
  cout<<"Enter height of the person:";
  cin>>height;
  float weight;
  cout<<"Enter weight of the person:";
 

    float bmi = weight / pow(height, 2); 
    return bmi; 
    
    
    if (bmi < 18.5) {
        cout << "underweight"; 
    }
  
    else if (bmi >= 18.5 && bmi < 24.9) {
        cout << "Healthy"; }
  
    else if (bmi >= 24.9 && bmi < 30) {
        cout << "overweight"; }
  
    else if (bmi >= 30 ) {
        cout << "Suffering from Obesity"; }
  
    return 0; 
} 
