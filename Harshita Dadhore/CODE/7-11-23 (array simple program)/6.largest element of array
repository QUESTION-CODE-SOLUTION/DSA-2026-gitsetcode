#include <iostream>
using namespace std;
 

int main(){
    int arr[5];
    
    int max = arr[0];  //assume largest element
    
     
    cout << "Enter elements of array: " ;
    for(int i=0;i<5;i++){
        cin>> arr[i] ; 
    } 
    for (int i = 1; i < 5; i++){
        if (arr[i] > max)
            max = arr[i];}
            cout<<"largest element is:"<<max;
 
    return 0;
}
