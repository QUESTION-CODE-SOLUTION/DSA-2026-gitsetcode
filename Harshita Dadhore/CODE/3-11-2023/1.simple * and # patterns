//1. simple pattern
//* * * * * 
//* * * * * 
//* * * * * 
//* * * * * 
//* * * * * 

#include<iostream>
using namespace std;
int main(){
     for( int i=1;i<=5;i++){
        for(int j=1;j<=5;j++){
            cout<<"* ";
        }
        cout<<endl;
    }
    return 0
}


-----------------------------------------------------------------------------------------------------------------------------------------
//2.simple star and hash pattern(row operation)
//* * * * * 
//# # # # # 
//* * * * * 
//# # # # # 
//* * * * * 

#include<iostream>
using namespace std;
int main(){
    
    for( int i=1;i<=5;i++){
        for(int j=1;j<=5;j++){
            if(i%2==0){
            cout<<"# ";}
            else{
            cout<<"* ";
            }
        }
        cout<<endl;
    }
    return 0;
}


----------------------------------------------------------------------------------------------------------------------------------------
3.//2.simple star and hash pattern(column  operation)
* # * # * 
* # * # * 
* # * # * 
* # * # * 
* # * # * 


#include<iostream>
using namespace std;
int main(){
    
    for( int i=1;i<=5;i++){
        for(int j=1;j<=5;j++){
            if(j%2==0){
            cout<<"# ";}
            else{
            cout<<"* ";
            }
        }
        cout<<endl;
    }
    return 0;
}

