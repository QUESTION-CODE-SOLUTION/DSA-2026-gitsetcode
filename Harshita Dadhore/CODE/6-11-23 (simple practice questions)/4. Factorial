#include <iostream>
using namespace std;
int main(){
    int num;
    cout<<"Enter a number:";
    cin>>num;
    int fact=1;

    if(num==0 || num==1){
        cout<<"Factorial is 1";
    }
    for(int i=1 ;i<=num;i++){
       fact=fact*i;


    }cout<<"Factorial of a number is:"<<fact;
    return 0;


}
