#include <iostream>
using namespace std;
int main()
{
    int arr[6],sum=0;
    cout << "Enter elements of array:" ;
    for(int i=0;i<5;i++){
        cin>>arr[i] ;
        sum+=arr[i];
    }
    cout << "Sum of elements of array is: "<<sum;
	return 0;
}

Output:
Enter elements of array:1 2 3 4 5 6
Sum of elements of array is: 21
