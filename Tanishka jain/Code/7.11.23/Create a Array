#include <iostream>
using namespace std;
int main() {
  int numbers[5];
 //taking input from user 
 cout << "Enter 5 numbers: " << endl;
  for (int i = 0; i < 5; ++i) {
    cin >> numbers[i];
  }
 //printing Array elemnts 
 cout << "The numbers are: ";
  for (int n = 0; n < 5; ++n) {
    cout << numbers[n] << "  ";
  }
  return 0;
}

Output=
Enter 5 numbers:
5
16
26
8
11
The numbers are: 5  16  26  8  11
