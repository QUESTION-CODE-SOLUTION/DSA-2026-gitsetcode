#include <iostream>
using namespace std;
int main (){
int n, t1 = 0, t2 = 1, nextnumber = 0;
 cout << "Enter the number of terms:";
 cin >> n;
  cout << "Fibonacci Series: ";
 for
     (int i = 1; i <= n; i++) {
     //Print first two numbers
     if(i == 1){
     cout << t1 << " , ";
     continue;
     }
     if(i == 2){
     cout << t2 << " , ";
     continue;
     }
     nextnumber = t1 + t2;
     t1 = t2;
     t2 = nextnumber;

     cout << nextnumber << " , ";
     }
    
return 0;
}

Output:
Enter the number of terms:6
Fibonacci Series: 0 , 1 , 1 , 2 , 3 , 5 , 
