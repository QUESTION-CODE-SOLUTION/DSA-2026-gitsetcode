#include <iostream>
#include <cmath>
using namespace std;
int main () {
    int weight , height ;
    cout << "Enter weight in pound:" << "";
    cin >> weight;
    
    cout << "Enter height in inches:" << "";
    cin >> height;
     double bmi = (weight / (pow(height, 2))) * 703;
cout << bmi;
}

Output:
Enter weight in pound:55
Enter height in inches:5.6
1546.6
