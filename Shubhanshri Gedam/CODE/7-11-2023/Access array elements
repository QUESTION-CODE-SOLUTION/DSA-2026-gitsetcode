#include <iostream>
using namespace std; 

int main() 
{ 

    int arr[5]; 

    // Inserting elements in an array 
    arr[0] = 10; 
    arr[1] = 15; 
    arr[2] = 20; 
    arr[3] = 25;
    arr[4] = 30;
  
    // Accessing and printing elements of the array 
    cout << "arr[0]: " << arr[0] << endl; 
    cout << "arr[1]: " << arr[1] << endl; 
    cout << "arr[2]: " << arr[2] << endl;
    cout << "arr[3]: " << arr[3] << endl; 
    cout << "arr[4]: " << arr[4] << endl; 


    return 0; 
}
