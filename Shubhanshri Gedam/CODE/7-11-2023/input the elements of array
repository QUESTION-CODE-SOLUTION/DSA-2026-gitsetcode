#include <iostream> 
using namespace std; 
int main() 
{ 
  int arr[5]; 
    cout << "Enter elements of array: " ;
    for(int i=0;i<5;i++){
        cin>> arr[i] ; 
    } 
    cout << "Array's elements: " ;
    for(int i=0;i<5;i++){
        cout << arr[i] << " "; 
    }
  return 0; 
}

//Enter elements of array:5
//10
//15
//20
//25
//Array's elements: 5 10 15 20 25
