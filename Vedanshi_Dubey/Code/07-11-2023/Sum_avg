#include <iostream>
using namespace std;
int main()
{
	float sum=0,avg=0;
    int arr[5],n;
    n = sizeof(arr) / sizeof(arr[0]);
    cout << "Enter elements of array: " ;
    for(int i=0;i<5;i++){
        cin>> arr[i] ;
        sum+=arr[i];
    }
    avg=sum/n;
    cout << "SUM: "<<sum<<endl;
    cout << "AVERAGE: "<<avg;
	return 0;
}
// Enter elements of array: 
// 1
// 3
// 4
// 2
// 5
// Sum: 15
// Average: 3
