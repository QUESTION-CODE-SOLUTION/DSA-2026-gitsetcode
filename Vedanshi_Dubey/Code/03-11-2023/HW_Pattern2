#include<iostream>
using namespace std;
int main(){
    int n,i,j;
    cout<<"Enter a number: ";
    cin>>n;
    for(i=1;i<=n;i++){
        for(j=1;j<=n;j++){
            if(i%2==0 && j%2==0)
            cout<<"# ";
            else
            cout<<"* ";
        }
        cout<<endl;
    }
}
// Enter a number: 4
// * * * * 
// * # * # 
// * * * * 
// * # * # 
