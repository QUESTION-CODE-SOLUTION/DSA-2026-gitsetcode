#include<iostream>
using namespace std;
int main(){
    int n,i,j;
    cout<<"Enter a number: ";
    cin>>n;
    for(i=n;i>=1;i--){
        for(j=1;j<=i;j++){
            cout<<j <<" ";
        }
        cout<<endl;
    }
}
// 1 2 3 4 5 
// 1 2 3 4
// 1 2 3
// 1 2
// 1
               //Another similar pattern
#include<iostream>
using namespace std;
int main(){
    int n,i,j;
    cout<<"Enter a number: ";
    cin>>n;
    for(i=n;i>=1;i--){
        for(j=1;j<=i;j++){
            cout<<i <<" ";
        }
        cout<<endl;
    }
}
// Enter a number: 6
// 6 6 6 6 6 6 
// 5 5 5 5 5
// 4 4 4 4
// 3 3 3
// 2 2
// 1 
