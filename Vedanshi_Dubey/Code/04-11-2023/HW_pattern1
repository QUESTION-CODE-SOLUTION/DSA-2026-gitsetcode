#include<iostream>
using namespace std;
int main(){
    int n,i,j,k;
        
        for(i=0;i<=4;i++){
        for(j=-4;j<=4;j++){
            if(j>-i&& j<i){
            cout<<" ";
        }
        else{
            cout<<"*";
        }
        }
        cout<<endl;
        }
}
