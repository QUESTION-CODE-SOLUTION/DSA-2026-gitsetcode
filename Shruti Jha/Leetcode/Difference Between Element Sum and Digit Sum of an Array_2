class Solution {
public:
    int differenceOfSum(vector<int>& nums) {
        int es=0,ds=0,a;
        for(int i=0;i<nums.size();i++){
            es+=nums[i];
            a=nums[i];
            if(a<10){ds+=a;}
            else{while(a!=0){
                ds+=a%10;
                a/=10;
            }}
        }
        return es>ds?es-ds:ds-es;
    }
};
