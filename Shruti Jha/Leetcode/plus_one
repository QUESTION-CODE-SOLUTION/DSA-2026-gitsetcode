class Solution {
public:
    vector<int> plusOne(vector<int>& digits) {
        int n=digits.size(),count=0;
        for(int i=n-1;i>=0;i--){
            if(digits[i]==9){
                count++;
                digits[i]=0;
            }
            else{
                 digits[i]++;
                 return digits;
            }
                
        }
        if(count==n){
            digits.insert(digits.begin(),1);
        }
        
        
        return digits;
    }
};
