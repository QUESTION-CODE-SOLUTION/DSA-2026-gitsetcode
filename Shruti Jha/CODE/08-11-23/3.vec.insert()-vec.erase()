#include<iostream>
#include<vector>
using namespace std; 
int main(){
   vector<int>vec={1,2,3,5,6};
   vec.insert(vec.begin()+3,4);
   for(int i=0;i<vec.size();i++){
      cout<<vec[i]<<" ";
   }
   cout<<endl;
   vec.erase(vec.begin()+5);
   for(int i=0;i<vec.size();i++){
      cout<<vec[i]<<" ";
   }
   return 0;
}


//1 2 3 4 5 6 
//1 2 3 4 5
