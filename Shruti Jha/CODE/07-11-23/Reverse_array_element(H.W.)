#include <iostream>
using namespace std;
int main()
{
    int n,temp,i,j;
    int arr[5] = {2, 4, 6, 8, 10};
    n = sizeof(arr) / sizeof(arr[0]);
    for ( i = 0, j = n - 1; i < n/2; i++, j--)  
    {     
        temp = arr[i];  
        arr[i] = arr[j];  
        arr[j] = temp;  
    }  
    cout << "Array's elements: " ;
    for(int i=0;i<n;i++){
        cout << arr[i] << " ";
    }
    return 0;
}
