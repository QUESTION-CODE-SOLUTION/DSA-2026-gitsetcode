#include <iostream> 
using namespace std; 
int main() 
{ 
	int arr[5]; 
	arr[0] = 2; 
	arr[1] = 4; 
	arr[2] = 6; 
  arr[3] = 8; 
	arr[4] = 10; 
	cout << "1st element: " << arr[0] << endl; 
	cout << "2nd element: " << arr[1] << endl; 
	cout << "3rd element: " << arr[2] << endl;
  cout << "4th element: " << arr[3] << endl; 
	cout << "5th element: " << arr[4] << endl; 
	return 0; 
}
