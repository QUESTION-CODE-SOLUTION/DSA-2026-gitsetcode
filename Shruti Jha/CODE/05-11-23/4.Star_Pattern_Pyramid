#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"Enter a number: ";
    cin>>n;
    for(int i=0;i<n;i++){
        for(int j=i;j<n;j++){
            cout<<" ";
        }
        for(int j=5-(2*i+1);j<5;j++){
            cout<<"*";
        }
        cout<<endl;
    }
}



// Enter a number: 5
//      *
//     ***
//   *****
//   *******
//  *********
