#include <iostream>
using namespace std;
int main()
{
    int n, count = 0;
    cout << "Enter a number: ";
    cin >> n;
    if(n<=1){cout << "Not Prime"; return 0;}
    for (int i = 2; i <= n / 2; i++)
    {
        if (n % i == 0)
        {
            count++;
            break;
        }
    }
    if (count !=0){
        cout << "Not Prime";
    }
    else
        cout << "Prime";
        return 0;
}






#include<iostream>
using namespace std;
int main(){
    int num;
    cin>>num;
    if(num<=1){cout << "Not Prime"; return 0;}
    int i=2;
    for(i=2;i<=num/2;i++){
        if(num%i==0){
            cout<<"Not Prime";
            break;
        }
    }
    if(i>=num/2+1){
        cout<<"Prime";
    }
    return 0;
}



